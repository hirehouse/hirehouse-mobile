{
  "name": "hirehouse-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "keywords": [
    "react",
    "expo",
    "template",
    "typescript",
    "nativebase"
  ],
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development expo start",
    "build:android:staging": "NODE_ENV=staging && eas build --platform android",
    "submit:android:staging": "NODE_ENV=staging && eas submit --platform android",
    "build:ios:staging": "NODE_ENV=staging && eas build --platform ios",
    "submit:ios:staging": "NODE_ENV=staging && eas submit --platform ios",
    "build:android:prod": "NODE_ENV=production && eas build --platform android",
    "submit:android:prod": "NODE_ENV=production && eas submit --platform android",
    "build:ios:prod": "NODE_ENV=production && eas build --platform ios",
    "submit:ios:prod": "NODE_ENV=production && eas submit --platform ios",
    "eas:logout": "eas logout",
    "eas:login": "eas login"
  },
  "dependencies": {
    "expo": "~42.0.0",
    "expo-status-bar": "~1.0.4",
    "native-base": "3.4.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-safe-area-context": "3.2.0",
    "react-native-svg": "12.1.1",
    "react-native-web": "~0.13.12",
    "react-router-native": "^6.3.0",
    "expo-network": "~3.2.0",
    "expo-notifications": "~0.12.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "typescript": "~4.0.0"
  },
  "bugs": {
    "url": "https://github.com/hirehouse/hirehouse-mobile.git/issues"
  },
  "homepage": "https://github.com/hirehouse/hirehouse-mobile.git#readme",
  "author": "Andreas Iosifelis",
  "private": true
}
